{
  "name": "@collab-ui/utils",
  "version": "2.0.1",
  "description": "Collab UI Utility functions.",
  "author": "Cisco Systems, Inc.",
  "homepage": "http://collab-ui.cisco.com/",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
