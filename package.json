    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "showdown": "^1.8.3"
  },
  "devDependencies": {
    "@collab-ui/release": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-lodash": "^2.4.5",
    "npm-watch": "^0.3.0"
